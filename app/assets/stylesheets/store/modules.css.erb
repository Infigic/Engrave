div#checkout #payment-methods li.last {
  display: inline;
  float: left;
}
div#checkout #checkout-summary {
  background-color: #dddddd;
  float: right;
  margin-top: 10px;
  padding: 20px;
  width: 180px;
}

#checkout .field br { display: none;}
#checkout .field input, #checkout .field select { float: left;}
#checkout .req { color: red; }

div#checkout form:after, .field:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
hr.space {clear: both;}
